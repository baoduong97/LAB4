<% @photos.each do |photo| %>
    <div class = "card">
        <div class = "card-header"> 
            <div id = "name"> 
                <%= photo.username %>
            </div>
            <div id = "time">
                <%= photo.avatar_url%>
            </div>
        </div>
        <%= image_tag photo.url, class: "card-img-top", alt: "photo" %>
            <div class="card-block">
                <p class="card-text">
                    <%= photo.caption %><br/>
                    <%= photo.likes_count%> likes
                </p>
            </div>
    </div>
    <% end %>